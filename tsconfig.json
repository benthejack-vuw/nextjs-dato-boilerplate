{
	"extends": "@catalyst/typescript-config/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@catalyst/ui/*": ["../../packages/ui/src/*"],
			"@catalyst/types/*": ["../../packages/types/*"]
		}
	},
	"include": ["src/**/*.ts", "src/**/*.tsx", "next-env.d.ts", "next.config.js", ".next/types/**/*.ts"],
	"exclude": ["node_modules", "../node_modules", "../../node_modules"]
}
